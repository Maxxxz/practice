  "description": "跑起几个基本的react项目。Webpack+React+ES6/7+ESlint+Sass",
  "main": "index.js",
  "scripts": {
    "precommit": "eslint ./js/ && eslint ./common",
    "pre": "npm install",
    "build_clear": "rimraf -rf build&& mkdir build",
    "dist_clear": "rimraf -rf dist&& mkdir dist",
    "mock": "json-server --watch ./mock/db.json",
    "dll": "npm run build_clear&&cross-env NODE_ENV=development webpack --config webpack.dll.js --progress --colors --profile",
    "start": "cross-env NODE_ENV=development node --max-old-space-size=8192 devServer.js",
    "testing": "npm run pre&& npm run dist_clear&&cross-env NODE_ENV=production webpack --config webpack.dist.js --devtool eval --progress -color --profile",
    "deploy": "npm run dist_clear&&cross-env NODE_ENV=production webpack --config webpack.dist.js --progress"
  },
