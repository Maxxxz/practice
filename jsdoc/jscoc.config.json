{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["./jsdoc.plugins/index.js"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true,
        "default": {
            "staticFiles": {
                "include": ["./dist/index.js"]
            }
        }
    },
    "opts": {
        "template": "jsdoc.template/minami",
        "encoding": "utf8",
        "destination": "out/",
        "recurse": true,
        "verbose": true
    }
}
