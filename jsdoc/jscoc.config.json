{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["./jsdoc.plugins/index.js"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "layoutFile": "./jsdoc.template/default/tmpl/layout.tmpl"
        }
    }
}
